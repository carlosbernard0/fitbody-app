{
  "expo": {
    "name": "template",
    "slug": "template-app",
    "scheme": "template-app",
    "version": "1.0.1",
    "sdkVersion": "52.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/sispro-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.carlosbernardo"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
